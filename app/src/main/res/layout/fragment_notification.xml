<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragment.NotificationFragment">

    <!-- TODO: Update blank fragment layout -->
   
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:orientation="vertical"
        tools:context=".CultureActivity">

        <Button
            android:id="@+id/go_to_music"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Explore Music"/>

        <Button
            android:id="@+id/go_to_FAQ"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Questions and Videos"/>

        <Button
            android:id="@+id/go_to_gallery"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Explore Galleries"/>

        <Button
            android:id="@+id/go_to_movies"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Explore Movies"/>

    </LinearLayout>


</FrameLayout>